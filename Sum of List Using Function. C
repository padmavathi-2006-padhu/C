#include <stdio.h>

int sumOfList(int arr[], int n) {
    int i, sum = 0;

    for (i = 0; i < n; i++) {
        sum = sum + arr[i];
    }
    return sum;
}

int main() {
    int arr[100], n, i, result;

    printf("Enter number of elements: ");
    scanf("%d", &n);

    printf("Enter elements:\n");
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    result = sumOfList(arr, n);

    printf("Sum of list elements = %d", result);

    return 0;
}
