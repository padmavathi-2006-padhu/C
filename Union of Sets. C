#include <stdio.h>

int main() {
    int set1[50], set2[50], unionSet[100];
    int n1, n2, i, j, k = 0, found;

    // Input first set
    printf("Enter number of elements in Set 1: ");
    scanf("%d", &n1);

    printf("Enter elements of Set 1:\n");
    for(i = 0; i < n1; i++) {
        scanf("%d", &set1[i]);
        unionSet[k++] = set1[i];   // Copy Set 1 into union
    }

    // Input second set
    printf("Enter number of elements in Set 2: ");
    scanf("%d", &n2);

    printf("Enter elements of Set 2:\n");
    for(i = 0; i < n2; i++) {
        scanf("%d", &set2[i]);

        found = 0;
        for(j = 0; j < n1; j++) {
            if(set2[i] == set1[j]) {
                found = 1;
                break;
            }
        }

        if(found == 0) {
            unionSet[k++] = set2[i];
        }
    }

    // Display union
    printf("Union of the two sets:\n");
    for(i = 0; i < k; i++) {
        printf("%d ", unionSet[i]);
    }

    return 0;
}
