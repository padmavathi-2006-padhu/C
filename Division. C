#include <stdio.h>

int main() {
    float a, b, result;

    printf("Enter two numbers: ");
    scanf("%f %f", &a, &b);

    if (b != 0) {
        result = a / b;
        printf("Division = %.2f", result);
    } else {
        printf("Error: Division by zero is not allowed");
    }

    return 0;
}
