#include <stdio.h>

int main() {
    FILE *fp;
    char ch;

    fp = fopen("file.txt", "r");   // open file in read mode

    if (fp == NULL) {
        printf("File not found!");
        return 1;
    }

    printf("File contents:\n");

    while ((ch = fgetc(fp)) != EOF) {
        printf("%c", ch);
    }

    fclose(fp);   // close file

    return 0;
}
