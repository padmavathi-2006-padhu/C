#include <stdio.h>

int main() {
    int a[50], b[50], u[100];
    int n1, n2, i, j, k = 0, flag;

    printf("Enter number of elements in Set A: ");
    scanf("%d", &n1);
    printf("Enter elements of Set A:\n");
    for (i = 0; i < n1; i++)
        scanf("%d", &a[i]);

    printf("Enter number of elements in Set B: ");
    scanf("%d", &n2);
    printf("Enter elements of Set B:\n");
    for (i = 0; i < n2; i++)
        scanf("%d", &b[i]);

    /* Copy elements of Set A to Union */
    for (i = 0; i < n1; i++) {
        u[k++] = a[i];
    }

    /* Add elements of Set B if not already present */
    for (i = 0; i < n2; i++) {
        flag = 0;
        for (j = 0; j < n1; j++) {
            if (b[i] == a[j]) {
                flag = 1;
                break;
            }
        }
        if (flag == 0) {
            u[k++] = b[i];
        }
    }

    printf("Union of two sets:\n");
    for (i = 0; i < k; i++) {
        printf("%d ", u[i]);
    }

    return 0;
}
